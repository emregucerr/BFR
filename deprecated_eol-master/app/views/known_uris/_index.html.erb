<% if @known_uris.empty? %>
  <li class="child"><%= I18n.t(:known_uris_empty_sorry) %></p>
<% else %>
  <% @known_uris.each do |ku| %>
    <li class="child attribute">
      <span title=<%=ku.uri%> class='name' data-id=<%=ku.id%> data-uri_type='<%= ku.allowed_units.empty? ? nil : "measurement"
        %>' data-has_values='<%= ku.has_values? ? "1" : nil%>'><%= raw(ku.name) %></span>
    </li>
  <% end %>
<% end %>
