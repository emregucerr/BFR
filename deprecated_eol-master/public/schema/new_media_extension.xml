<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ipt.gbif.org/extension/"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://ipt.gbif.org/extension/ http://gbrds.gbif.org/resources/schemas/extension.xsd"
            title="EOL Media Extension 1.0"
            name="EOL Media Extension 1.0"
            namespace="http://www.eol.org/schema/transfer#">

    <property name="identifier"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/identifier"
              required="true"/>

    <property name="taxonID"
              namespace="http://rs.tdwg.org/dwc/terms/"
              qualName="http://rs.tdwg.org/dwc/terms/taxonID"/>

    <property name="scientificName"
              namespace="http://rs.tdwg.org/dwc/terms/"
              qualName="http://rs.tdwg.org/dwc/terms/scientificName"/>

    <property name="vernacularName"
              namespace="http://rs.tdwg.org/dwc/terms/"
              qualName="http://rs.tdwg.org/dwc/terms/vernacularName"/>

    <property name="type"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/type"
              required="true"/>

    <property name="subtype"
              namespace="http://rs.tdwg.org/audubon_core/"
              qualName="http://rs.tdwg.org/audubon_core/subtype"/>

    <property name="format"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/format"/>

    <!-- SPM or EOL -->
    <property name="CVterm"
              namespace="http://iptc.org/std/Iptc4xmpExt/1.0/xmlns/"
              qualName="http://iptc.org/std/Iptc4xmpExt/1.0/xmlns/CVterm"/>

    <property name="title"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/title"/>

    <property name="description"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/description"/>

    <property name="accessURI"
              namespace="http://rs.tdwg.org/ac/terms/"
              qualName="http://rs.tdwg.org/ac/terms/accessURI"/>

    <property name="thumbnailURL"
              namespace="http://eol.org/schema/media/"
              qualName="http://eol.org/schema/media/thumbnailURL"/>

    <property name="furtherInformationURL"
              namespace="http://rs.tdwg.orgif you terms/"
              qualName="http://rs.tdwg.org/ac/terms/furtherInformationURL"/>

    <property name="CreateDate"
              namespace="http://ns.adobe.com/xap/1.0/"
              qualName="http://ns.adobe.com/xap/1.0/CreateDate"/>

    <property name="modified"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/modified"/>

    <property name="available"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/available"/>

    <property name="language"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/language"/>

    <!-- http://partners.adobe.com/public/developer/en/xmp/sdk/XMPspecification.pdf -->
    <!-- A rating of the media resources, provided by users or editors, with -1 defining 'rejected', '0' defining 'unrated', and '1' (worst) to '5' (best). -->
    <property name="rating"
              namespace="http://ns.adobe.com/xap/1.0/"
              qualName="http://ns.adobe.com/xap/1.0/Rating"/>

    <property name="audience"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/audience"/>

    <property name="UsageTerms"
              namespace="http://ns.adobe.com/xap/1.0/rights/"
              qualName="http://ns.adobe.com/xap/1.0/rights/UsageTerms"
              required="true"/>

    <property name="rights"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/rights"/>

    <property name="Owner"
              namespace="http://ns.adobe.com/xap/1.0/rights/"
              qualName="http://ns.adobe.com/xap/1.0/rights/Owner"/>

    <property name="bibliographicCitation"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/bibliographicCitation"/>

    <property name="publisher"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/publisher"/>

    <property name="contributor"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/contributor"/>

    <property name="creator"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/creator"/>

    <property name="provider"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/provider"/>

    <property name="subjectPart"
              namespace="http://rs.tdwg.org/ac/terms/"
              qualName="http://rs.tdwg.org/ac/terms/subjectPart"/>

    <property name="lifeStage"
              namespace="http://rs.tdwg.org/dwc/terms/"
              qualName="http://rs.tdwg.org/dwc/terms/lifeStage"/>

    <property name="subjectOrientation"
              namespace="http://rs.tdwg.org/ac/terms/"
              qualName="http://rs.tdwg.org/ac/terms/subjectOrientation"/>

    <property name="LocationCreated"
              namespace="http://iptc.org/std/Iptc4xmpExt/1.0/xmlns/"
              qualName="http://iptc.org/std/Iptc4xmpExt/1.0/xmlns/LocationCreated"/>

    <property name="lat"
              namespace="http://www.w3.org/2003/01/geo/wgs84_pos#"
              qualName="http://www.w3.org/2003/01/geo/wgs84_pos#lat"/>

    <property name="long"
              namespace="http://www.w3.org/2003/01/geo/wgs84_pos#"
              qualName="http://www.w3.org/2003/01/geo/wgs84_pos#long"/>

    <property name="alt"
              namespace="http://www.w3.org/2003/01/geo/wgs84_pos#"
              qualName="http://www.w3.org/2003/01/geo/wgs84_pos#alt"/>

    <property name="reference"
              namespace="http://eol.org/schema/media/"
              qualName="http://eol.org/schema/media/reference"/>
</extension>