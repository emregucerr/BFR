<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="owl_view.xsl"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY dwc "http://rs.tdwg.org/dwc/terms/" >
    <!ENTITY bibo "http://purl.org/ontology/bibo/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY reference "http://www.eol.org/schema/reference#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF xmlns="&reference;"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:terms="http://purl.org/dc/terms/"
   xmlns:reference="http://www.eol.org/schema/reference#"
   xmlns:dwc="http://rs.tdwg.org/dwc/terms/"
   xmlns:bibo="http://purl.org/ontology/bibo/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   
  <owl:Ontology rdf:about="&reference;">
    <rdfs:label></rdfs:label>
    <rdfs:comment>New EOL Reference Ontology</rdfs:comment>
  </owl:Ontology>
  
  <!-- &reference;Document -->
  <owl:Class rdf:ID="Document">
     <rdfs:label></rdfs:label>
     <rdfs:comment></rdfs:comment>
     <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
     <owl:equivalentClass rdf:resource="http://purl.org/ontology/bibo/Document"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="referenceID">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <rdfs:subPropertyOf rdf:resource="&terms;identifier"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="fullReference">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <rdfs:subPropertyOf rdf:resource="&terms;bibliographicCitation"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="authorList">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;authorList"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="editorList">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;editorList"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="created">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&terms;created"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="title">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&terms;title"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="pages">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;pages"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="pagesStart">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;pagesStart"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="pageEnd">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;pageEnd"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="volume">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;volume"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="edition">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;edition"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="publisher">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&terms;publisher"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="language">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&terms;language"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="uri">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;uri"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="doi">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;doi"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="isbn">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;isbn"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="issn">
    <rdfs:label></rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:domain rdf:resource="&reference;Document"/>
    <owl:equivalentProperty rdf:resource="&bibo;issn"/>
  </owl:ObjectProperty>
  
</rdf:RDF>